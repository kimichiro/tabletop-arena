{
    "name": "@tabletop-arena/tictactoe-svelte",
    "version": "0.0.1",
    "type": "module",
    "private": true,
    "engines": {
        "node": ">=20"
    },
    "scripts": {
        "start": "export $(cat .env.local | xargs) && node build/index.js",
        "dev": "vite dev",
        "build": "vite build",
        "preview": "vite preview",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "lint": "prettier --write . && eslint .",
        "test": "vitest run"
    },
    "dependencies": {
        "colyseus.js": "^0.15.25",
        "eventemitter3": "^5.0.1"
    },
    "devDependencies": {
        "@sveltejs/adapter-node": "^5.2.0",
        "@sveltejs/kit": "^2.5.18",
        "@sveltejs/vite-plugin-svelte": "^3.1.1",
        "@tabletop-arena/eslint-config": "^0.0.0",
        "@tabletop-arena/game-client": "^0.0.0",
        "@tabletop-arena/tictactoe": "^0.0.0",
        "@tabletop-arena/typescript-config": "^0.0.0",
        "@tailwindcss/typography": "^0.5.13",
        "@types/eslint": "^8.56.10",
        "@typescript-eslint/eslint-plugin": "^7.16.0",
        "@typescript-eslint/parser": "^7.16.0",
        "@vitest/coverage-v8": "^2.0.3",
        "daisyui": "^4.12.10",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-svelte": "^2.42.0",
        "nanoid": "^5.0.7",
        "prettier": "^3.3.3",
        "prettier-plugin-svelte": "^3.2.5",
        "svelte": "^4.2.18",
        "svelte-check": "^3.8.4",
        "tailwindcss": "^3.4.4",
        "tslib": "^2.6.3",
        "typescript": "^5.5.3",
        "vite": "^5.3.3",
        "vitest": "^2.0.3"
    }
}
